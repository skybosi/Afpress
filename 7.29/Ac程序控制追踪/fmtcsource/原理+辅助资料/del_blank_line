#include<stdio.h>
#include<string.h>
// #include"right_n_str.c"
char *right_n_str(char *dst, char *src, int n);
void del_blank_line(char *str);
int main()
{
	char *str = "	     	      	vguhvhh  jui  	";
	del_blank_line(str);
	return 0;
}

void del_blank_line(char *str)
{
	int i = 0;
	char str1[strlen(str)];
	while ((str[i] == ' ') || (str[i] == '\t'))
		 i++;
	printf("%d %d\n", i, strlen(str) - i);
	right_n_str(str1, str, strlen(str) - i);
	printf("%s", str1);
}

char *right_n_str(char *dst, char *src, int n)
{
	char *p = src;
	char *q = dst;
	int len = strlen(src);
	if (n > len)
		n = len;
	p += (len - n);				// 从右边第n个字符开始
	while (*(q++) = *(p++));
	return dst;
}