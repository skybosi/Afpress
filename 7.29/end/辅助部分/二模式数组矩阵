#include<stdio.h>
int a[8][7] = { {0, 512, 1024, 2048, 4096, 8192, 16384},
{4, 516, 1032, 2064, 4128, 8256, 16512},
{8, 520, 1040, 2080, 4160, 8320, 16640},
{16, 528, 1056, 2112, 4224, 8448, 16896},
{32, 544, 1088, 2176, 4352, 8704, 17408},
{64, 576, 1152, 2304, 4608, 9216, 18432},
{128, 640, 1280, 2560, 5120, 10240, 20480},
{256, 768, 1536, 3072, 6144, 12288, 24576}
};

int b[7][8] = { {0, 4, 8, 16, 32, 64, 128, 256},
{512, 516, 520, 528, 544, 576, 640, 768},
{1024, 1032, 1040, 1056, 1088, 1152, 1280, 1536},
{2048, 2064, 2080, 2112, 2176, 2304, 2560, 3072},
{4096, 4128, 4160, 4224, 4352, 4608, 5120, 6144},
{8192, 8256, 8320, 8448, 8704, 9216, 10240, 12288},
{16384, 16512, 16640, 16896, 17408, 18432, 20480, 24576}
};

int main()
{
	for (int j = 0; j < 8; j++)
		printf("%6d", j);
	printf("\n");
	for (int i = 0; i < 7; i++)
	{
		printf("%d  ", i);
		for (int j = 0; j < 8; j++)
		{
			printf("%5d ", a[j][i]);
		}
		printf("\n");
	}
	printf("\n%d, %d", b[4][1], b[2][6]);
	return 0;
}